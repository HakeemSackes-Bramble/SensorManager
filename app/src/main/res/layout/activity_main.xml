<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="nyc.c4q.hakeemsackes_bramble.sensormanager.GameActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:measureAllChildren="true">
        <nyc.c4q.hakeemsackes_bramble.sensormanager.tilt_based_game.tilt_views.BackgroundView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/background_view"/>

        <nyc.c4q.hakeemsackes_bramble.sensormanager.tilt_based_game.tilt_views.PlayerCharacterView
            android:id="@+id/player_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>


    </FrameLayout>
</LinearLayout>
